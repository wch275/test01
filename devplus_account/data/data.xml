<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data noupdate="1">

		<record model="account.payment.mode" id="account_payment_mode_espece">
			<field name="name">Espéce</field>
			<field name="code">CHE</field>
			<field name="type">cash</field>
		</record>

		<record model="account.payment.mode" id="account_payment_mode_cheque">
			<field name="name">Chèque</field>
			<field name="code">CHE</field>
			<field name="type">bank</field>
		</record>

		<record model="account.payment.mode" id="account_payment_mode_virement">
			<field name="name">Virement</field>
			<field name="code">VIR</field>
			<field name="type">bank</field>
		</record>

		<record model="account.payment.mode" id="account_payment_mode_traite">
			<field name="name">Traite</field>
			<field name="code">TRA</field>
			<field name="type">bank</field>
		</record>

		<record id="seq_type_bordereau_remise" model="ir.sequence.type">
			<field name="name">Bordereau de remise</field>
			<field name="code">account.bordereau_remise</field>
		</record>

		<record id="seq_bordereau_remise" model="ir.sequence">
			<field name="name">Bordereau de remise</field>
			<field name="code">account.bordereau_remise</field>
			<field eval="3" name="padding" />
			<field name="prefix">%(year)s/</field>
		</record>

	</data>
</openerp>
    